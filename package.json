{
	"name": "Clockify",
	"version": "2.11.9",
	"description": "Clockify App 100% Free Time Tracker",
	"main": "./main.js",
	"productName": "Clockify",
	"homepage": "https://app.clockify.me/",
	"scripts": {
		"clean.build.chrome": "node tools/clean-build-script.js chrome",
		"clean.build.chrome.dev": "node tools/clean-build-script.js chrome.dev",
		"clean.build.firefox": "node tools/clean-build-script.js firefox",
		"clean.build.firefox.dev": "node tools/clean-build-script.js firefox.dev",
		"delete.integrations": "node tools/clean-build-script.js www/chrome/integrations",
		"delete.contentScripts": "node tools/clean-build-script.js www/chrome/contentScripts",
		"compile.css.chrome": "sass sass/main-integration.scss chrome.dev/styles/main-integration.css --style compressed",
		"compile.css.firefox": "sass sass/main-integration.scss firefox.dev/styles/main-integration.css --style compressed",
		"compile.prod.css.chrome": "sass sass/main-integration.scss chrome/styles/main-integration.css --style compressed",
		"compile.prod.css.firefox": "sass sass/main-integration.scss firefox/styles/main-integration.css --style compressed",
		"compile.dev.chrome": "npm run clean.build.chrome.dev && npm run compile.css.chrome && TARGET='chrome.dev' webpack --watch",
		"compile.dev.chrome.nowatch": "npm run clean.build.chrome.dev && npm run compile.css.chrome && TARGET='chrome.dev' webpack",
		"compile.dev.firefox": "npm run clean.build.firefox.dev && npm run compile.css.firefox && TARGET='firefox.dev' webpack --watch",
		"compile.dev.firefox.nowatch": "npm run clean.build.firefox.dev && npm run compile.css.firefox && TARGET='firefox.dev' webpack",
		"compile.prod.chrome": "npm run clean.build.chrome && npm run compile.prod.css.chrome && NODE_ENV=prod TARGET=chrome webpack",
		"compile.prod.firefox": "npm run clean.build.firefox && npm run compile.prod.css.firefox && NODE_ENV=prod TARGET=firefox webpack",
		"compile.stage.chrome": "npm run clean.build.chrome && npm run compile.css.chrome && NODE_ENV=stage TARGET=chrome webpack --watch",
		"compile.stage.firefox": "npm run clean.build.firefox && npm run compile.css.firefox && NODE_ENV=stage TARGET=firefox webpack",
		"compile.dev": "npm run compile.dev.chrome & npm run compile.dev.firefox",
		"compile.prod": "npm run compile.prod.chrome & npm run compile.prod.firefox",
		"compile": "npm run compile.dev & npm run compile.prod",
		"test": "echo \"Error: no test specified\" && exit 1",
		"generate.locale": "node tools/generate-locale.js",
		"use.remote.selectors": "node tools/change-selectors-source.js remote",
		"use.local.selectors": "node tools/change-selectors-source.js local"
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@fontsource/roboto": "^4.5.8",
		"@popperjs/core": "^2.11.0",
		"antd": "^4.21.7",
		"axios": "^1.7.5",
		"child_process": "^1.0.2",
		"date-fns": "^2.28.0",
		"express": "^4.15.3",
		"form-data": "^2.3.3",
		"jsonwebtoken": "9.0.0",
		"lodash": ">=4.17.13",
		"moment": "^2.18.1",
		"moment-duration-format": "^2.2.2",
		"moment-timezone": "^0.5.21",
		"react": "^18.2.0",
		"react-autocomplete": "^1.8.1",
		"react-datepicker": "^4.6.0",
		"react-dom": "^18.2.0",
		"react-http-request": "^2.0.0",
		"react-modal": "^3.15.1",
		"react-onclickoutside": "^6.12.1",
		"react-popper": "^2.2.5",
		"react-shadow": "^19.0.3",
		"url-parse": "^1.4.3",
		"zustand": "^4.4.7"
	},
	"devDependencies": {
		"@babel/core": "^7.17.8",
		"@babel/preset-env": "^7.16.11",
		"@babel/preset-react": "^7.16.7",
		"@babel/preset-typescript": "^7.24.1",
		"@types/react": "^18.2.79",
		"assert": "npm:assert",
		"babel-core": "^7.0.0-bridge.0",
		"babel-loader": "^8.2.5",
		"buffer": "^6.0.3",
		"copy-webpack-plugin": "^11.0.0",
		"core-js": "^3.24.1",
		"crypto": "npm:crypto-browserify",
		"css-loader": "^5.2.7",
		"file-loader": "^6.2.0",
		"fs-extra": "^7.0.1",
		"http": "npm:stream-http",
		"https": "npm:https-browserify",
		"node-polyfill-webpack-plugin": "^2.0.1",
		"os": "npm:os-browserify",
		"resolve-url-loader": "^5.0.0",
		"rimraf": "^2.6.2",
		"sass": "^1.53.0",
		"sass-loader": "^13.0.2",
		"stream": "npm:stream-browserify",
		"stream-browserify": "^3.0.0",
		"style-loader": "^3.3.1",
		"to-string-loader": "^1.2.0",
		"ts-loader": "^9.5.1",
		"typescript": "^5.4.5",
		"webpack": "^5.74.0",
		"webpack-cli": "^4.10.0",
		"webpack-dev-middleware": "5.3.4",
		"xml2js": "0.6.2"
	}
}
